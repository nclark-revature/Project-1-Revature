<apex:page standardController="Property__c">
    <apex:form id="addPropertyForm">
        <apex:pageBlock title="Add Your Property">
            <apex:pageBlockSection >
                <apex:inputField value="{!Property__c.Apartment_Name__c}"/>
                <apex:inputField value="{!Property__c.Name}"/>
                <apex:inputField value="{!Property__c.Unit_Type__c}"/>
                <apex:inputField value="{!Property__c.Description__c}"/>
                <apex:inputField value="{!Property__c.Year_Build__c}"/>
                <apex:inputField value="{!Property__c.Street_Address__c}"/>
                <apex:inputField value="{!Property__c.City__c}"/>
                <apex:inputField value="{!Property__c.State__c}"/>
                <apex:inputField value="{!Property__c.Zip_Code__c}"/>
                <apex:inputField value="{!Property__c.Status__c}"/>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton action="{!save}" value="Save"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>